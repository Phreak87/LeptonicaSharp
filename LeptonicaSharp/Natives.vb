			ByVal pixa as IntPTR, 
			ByVal outwidth as Integer, 
			ByVal newrow as Integer, 
			ByVal space as Integer, 
			ByVal linewidth as Integer, 
			ByVal bmf as IntPTR, 
			ByVal textstr as String, 
			ByVal val as UInteger, 
			ByVal location as Integer) as Integer
End Function

